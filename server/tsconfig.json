{
    "compilerOptions": {
        "strict": true,
        "target": "ESNext",
        "module": "ESNext",
        "moduleResolution": "Node",
        "resolveJsonModule": true,
        "esModuleInterop": true,
        "paths": {
            "server": [
                "./src/index.ts"
            ],
            "types": [
                "../types.ts"
            ],
            "out/*": [
                "./out/*"
            ]
        }
    }
}